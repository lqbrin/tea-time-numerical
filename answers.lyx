\begin_layout Plain Layout

function roots = newthornall(c,tol,N,x0)
\end_layout

\begin_layout Plain Layout

  n=length(c)-1;
\end_layout

\begin_layout Plain Layout

  for i=1:n-2
\end_layout

\begin_layout Plain Layout

    res=newtonhorner(c,x0,tol,N)
\end_layout

\begin_layout Plain Layout

    roots(i)=res;
\end_layout

\begin_layout Plain Layout

    x0=roots(i);
\end_layout

\begin_layout Plain Layout

    c=deflate(c,x0);
\end_layout

\begin_layout Plain Layout

  end%for
\end_layout

\begin_layout Plain Layout

  [roots(n-1),roots(n)]=quadraticRoots(c(3),c(2),c(1));
\end_layout

\begin_layout Plain Layout

end%function
