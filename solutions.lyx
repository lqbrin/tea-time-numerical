  -10  -19
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

c =
\end_layout

\begin_layout Plain Layout

  -9   0
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

d =
\end_layout

