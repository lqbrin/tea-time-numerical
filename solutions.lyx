\begin_layout Plain Layout

####################################################
\end_layout

\begin_layout Plain Layout

function integral = compositeTrapezoidal(f,a,b,n)
\end_layout

\begin_layout Plain Layout

  h = (b-a)/n;
\end_layout

\begin_layout Plain Layout

  s = 0;
\end_layout

\begin_layout Plain Layout

  for i = 1:n-1
\end_layout

\begin_layout Plain Layout

    s = s + f(a+i*h);
\end_layout

\begin_layout Plain Layout

  end#for
\end_layout

\begin_layout Plain Layout

  integral = h*(f(a)+2*s+f(b))/2;
\end_layout

\begin_layout Plain Layout

end#function
\end_layout

