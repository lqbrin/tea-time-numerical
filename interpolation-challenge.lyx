\begin_layout Plain Layout

% Purpose: Calculate values of the fractal interpolating %
\end_layout

\begin_layout Plain Layout

%        function, f, passing through                    %
\end_layout

\begin_layout Plain Layout

%        (0,f_0), (alpha,f_alpha), and (1,f_1),          %
\end_layout

\begin_layout Plain Layout

%        its derivative and its integral.
                %
\end_layout

\begin_layout Plain Layout

% INPUT: value at which to evaluate, x; array of values, %
\end_layout

\begin_layout Plain Layout

%        f = [f_0,f_alpha,f_1]; alpha; scaling factors   %
\end_layout

\begin_layout Plain Layout

%        d1 and d2.
                                      %
\end_layout

\begin_layout Plain Layout

% OUTPUT: y=f'(x); yy=f(x); yyy=F(x).
                    %
\end_layout

\begin_layout Plain Layout

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\end_layout

\begin_layout Plain Layout

function [y,yy,yyy] = fractalInterpolator(x,f,alpha,d1,d2)
\end_layout

\begin_layout Plain Layout

  f1=f(1)*(1-d1);
\end_layout

\begin_layout Plain Layout

  c1=f(2)-d1*f(3)-f1;
\end_layout

\begin_layout Plain Layout

  f2=f(2)-d2*f(1);
\end_layout

\begin_layout Plain Layout

  c2=(1-d2)*f(3)-f2;
\end_layout

\begin_layout Plain Layout

  F1=(alpha*(f1+c1/2)+(1-alpha)*(f2+c2/2))/(1-(1-alpha)*d2-alpha*d1);
\end_layout

\begin_layout Plain Layout

  FA=alpha*(f1+c1/2+d1*F1);
\end_layout

\begin_layout Plain Layout

  l=0;
\end_layout

\begin_layout Plain Layout

  r=1;
\end_layout

\begin_layout Plain Layout

  a=[];
\end_layout

\begin_layout Plain Layout

  if (alpha>1/2)
\end_layout

\begin_layout Plain Layout

    its=floor(log(10^-16)/log(alpha));
\end_layout

\begin_layout Plain Layout

  else
\end_layout

\begin_layout Plain Layout

    its=floor(log(10^-16)/log(1-alpha));
\end_layout

\begin_layout Plain Layout

  end%if
\end_layout

\begin_layout Plain Layout

  for i=1:its
\end_layout

\begin_layout Plain Layout

    if (alpha>1/2)
\end_layout

\begin_layout Plain Layout

      h = (r-l)*alpha;
\end_layout

\begin_layout Plain Layout

      m = l+h;
\end_layout

\begin_layout Plain Layout

    else
\end_layout

\begin_layout Plain Layout

      h = (r-l)*(1-alpha);
\end_layout

\begin_layout Plain Layout

      m = r-h;
\end_layout

\begin_layout Plain Layout

    end%if
\end_layout

\begin_layout Plain Layout

    if (x<m)
\end_layout

\begin_layout Plain Layout

      a(i)=0;
\end_layout

\begin_layout Plain Layout

      r=m;
\end_layout

\begin_layout Plain Layout

    else
\end_layout

\begin_layout Plain Layout

      a(i)=1;
\end_layout

\begin_layout Plain Layout

      l=m;
\end_layout

\begin_layout Plain Layout

    end%if
\end_layout

\begin_layout Plain Layout

  end%for
\end_layout

\begin_layout Plain Layout

  x=0;
\end_layout

\begin_layout Plain Layout

  y=c1/(alpha-d1);
\end_layout

\begin_layout Plain Layout

  yy=f(1);
\end_layout

\begin_layout Plain Layout

  yyy=0;
\end_layout

\begin_layout Plain Layout

  for i=its:-1:1
\end_layout

\begin_layout Plain Layout

    if (a(i)==0)
\end_layout

\begin_layout Plain Layout

      y=(c1+d1*y)/alpha;
\end_layout

\begin_layout Plain Layout

      yy=c1*x+d1*yy+f1;
\end_layout

\begin_layout Plain Layout

      yyy=alpha*(f1*x+c1/2*x*x+d1*yyy);
\end_layout

\begin_layout Plain Layout

      x=alpha*x;
\end_layout

\begin_layout Plain Layout

    else
\end_layout

\begin_layout Plain Layout

      y=(c2+d2*y)/(1-alpha);
\end_layout

\begin_layout Plain Layout

      yy=c2*x+d2*yy+f2;
\end_layout

\begin_layout Plain Layout

      yyy=FA+(1-alpha)*(f2*x+c2/2*x*x+d2*yyy);
\end_layout

\begin_layout Plain Layout

      x=alpha+(1-alpha)*x;
\end_layout

\begin_layout Plain Layout

    end%if
\end_layout

\begin_layout Plain Layout

  end%for
\end_layout

\begin_layout Plain Layout

end%function 
\end_layout

