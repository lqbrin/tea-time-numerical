\begin_layout Standard
\noindent
\begin_inset Index idx
status collapsed

\begin_layout Plain Layout
secant method ! analysis
\end_layout

\end_inset

Suppose 
\begin_inset Formula $g$
\end_inset

 is a function with root 
\begin_inset Formula $\hat{x}$
\end_inset

, 
\begin_inset Formula $g'(\hat{x})\neq0$
\end_inset

, 
\begin_inset Formula $g''(\hat{x})\neq0$
\end_inset

, and 
\begin_inset Formula $g'''(x)$
\end_inset

 exists in a neighborhood of 
\begin_inset Formula $\hat{x}$
\end_inset

.
 Let 
\begin_inset Formula $x_{0},x_{1},x_{2},\ldots$
\end_inset

 be a sequence derived from the secant method (
\begin_inset Formula $x_{n+1}=x_{n}-g(x_{n})\frac{x_{n}-x_{n-1}}{g(x_{n})-g(x_{n-1})}$
\end_inset

 for all 
\begin_inset Formula $k\geq2$
\end_inset

) such that 
\begin_inset Formula ${\displaystyle \lim_{k\to\infty}x_{k}=\hat{x}}$
\end_inset

.
 Define 
\begin_inset Formula $e_{n}=x_{n}-\hat{x}$
\end_inset

 so 
\begin_inset Formula $x_{n}=\hat{x}+e_{n}$
\end_inset

.
 Making this substitution into 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:secantMethod"

\end_inset

 we have 
\begin_inset Formula 
\begin{equation}
e_{n+1}=e_{n}-g(\hat{x}+e_{n})\frac{e_{n}-e_{n-1}}{g(\hat{x}+e_{n})-g(\hat{x}+e_{n-1})}.\label{eq:secantOrderPart1}
\end{equation}

\end_inset

Taylor's theorem allows 
\begin_inset Formula $g(\hat{x}+e_{k})=g(\hat{x})+e_{k}g'(\hat{x})+\frac{1}{2}e_{k}^{2}g''(\hat{x})+O(e_{k}^{3})$
\end_inset

.
 Noting that 
\begin_inset Formula $g(\hat{x})=0$
\end_inset

 and substituting into 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:secantOrderPart1"

\end_inset

,
\begin_inset Formula 
\begin{eqnarray}
e_{n+1} & = & e_{n}-(e_{n}-e_{n-1})\frac{e_{n}g'(\hat{x})+\frac{1}{2}e_{n}^{2}g''(\hat{x})+O(e_{n}^{3})}{(e_{n}-e_{n-1})g'(\hat{x})+\frac{1}{2}(e_{n}^{2}-e_{n-1}^{2})g''(\hat{x})+O(e_{n-1}^{3})}\nonumber \\
 & = & e_{n}-\frac{e_{n}+\frac{e_{n}^{2}g''(\hat{x})}{2g'(\hat{x})}+O(e_{n}^{3})}{1+\frac{(e_{n}+e_{n-1})g''(\hat{x})}{2g'(\hat{x})}+\frac{O(e_{n-1}^{3})}{(e_{n}-e_{n-1})}}\nonumber \\
 & = & \frac{e_{n}\left(1+\frac{(e_{n}+e_{n-1})g''(\hat{x})}{2g'(\hat{x})}+\frac{O(e_{n-1}^{3})}{(e_{n}-e_{n-1})}\right)-\left(e_{n}+\frac{e_{n}^{2}g''(\hat{x})}{2g'(\hat{x})}+O(e_{n}^{3})\right)}{1+\frac{(e_{n}+e_{n-1})g''(\hat{x})}{2g'(\hat{x})}+\frac{O(e_{n-1}^{3})}{(e_{n}-e_{n-1})}}\nonumber \\
 & = & \frac{e_{n}e_{n-1}\frac{g''(\hat{x})}{2g'(\hat{x})}+\frac{e_{n}}{e_{n}-e_{n-1}}O(e_{n-1}^{3})+O(e_{n}^{3})}{1+\frac{(e_{n}+e_{n-1})g''(\hat{x})}{2g'(\hat{x})}+\frac{O(e_{n-1}^{3})}{(e_{n}-e_{n-1})}}.\label{eq:secantOrderPart2}
\end{eqnarray}

\end_inset

Using equality 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:secantOrderPart2"

\end_inset

 to find a value 
\begin_inset Formula $\alpha$
\end_inset

 for which 
\begin_inset Formula $\lim_{n\to\infty}\frac{|\hat{x}-x_{n+1}|}{|\hat{x}-x_{n}|^{\alpha}}=\lambda\neq0$
\end_inset

, we have
\begin_inset Formula 
\begin{eqnarray*}
\lim_{n\to\infty}\frac{|\hat{x}-x_{n+1}|}{|\hat{x}-x_{n}|^{\alpha}} & = & \lim_{n\to\infty}\frac{|e_{n+1}|}{|e_{n}|^{\alpha}}\\
 & = & \lim_{n\to\infty}\left|\frac{e_{n}^{1-\alpha}e_{n-1}\frac{g''(\hat{x})}{2g'(\hat{x})}+\frac{e_{n}^{1-\alpha}}{e_{n}-e_{n-1}}O(e_{n-1}^{3})+O(e_{n}^{3-\alpha})}{1+\frac{(e_{n}+e_{n-1})g''(\hat{x})}{2g'(\hat{x})}+\frac{O(e_{n-1}^{3})}{(e_{n}-e_{n-1})}}\right|\\
 & = & \lambda\neq0.
\end{eqnarray*}

\end_inset

But 
\begin_inset Formula $\lim_{n\to\infty}e_{n}=\lim_{n\to\infty}e_{n-1}=0$
\end_inset

.
 Hence, 
\begin_inset Formula $\lim_{n\to\infty}e_{n}^{1-\alpha}e_{n-1}$
\end_inset

 must not be 0 or divergent, for if it were, 
\begin_inset Formula $\lim_{n\to\infty}\frac{|\hat{x}-x_{n+1}|}{|\hat{x}-x_{n}|^{\alpha}}$
\end_inset

 would be 0 or divergent, respectively.
 Consequently, there is a positive constant 
\begin_inset Formula $C$
\end_inset

 such that 
\begin_inset Formula $\lim_{n\to\infty}|e_{n}^{1-\alpha}e_{n-1}|=\lim_{n\to\infty}|e_{n+1}^{1-\alpha}e_{n}|=C\Rightarrow\lim_{n\to\infty}|e_{n+1}e_{n}^{1/(1-\alpha)}|=C^{1/(1-\alpha)}$
\end_inset

.
 Now we have 
\begin_inset Formula 
\[
\lim_{n\to\infty}\frac{|e_{n+1}|}{|e_{n}|^{\alpha}}=\lambda\neq0\mbox{ and }\lim_{n\to\infty}\frac{|e_{n+1}|}{|e_{n}|^{1/(\alpha-1)}}=C^{1/(1-\alpha)}\neq0.
\]

\end_inset

Since the order of convergence of a sequence is unique (Exercise 
\begin_inset CommandInset ref
LatexCommand ref
reference "enu:orderOfConvergenceIsUnique"

\end_inset

 of section 
\begin_inset CommandInset ref
LatexCommand ref
reference "sec:Speed"

\end_inset

) it must be that 
\begin_inset Formula $\alpha=1/(\alpha-1)$
\end_inset

 or 
\begin_inset Formula $\alpha^{2}-\alpha-1=0$
\end_inset

.
 The quadratic formula supplies the desired result.
\end_layout

